{}
{
	<MAIN>
	|<LBRACE>
	|<RBRACE>
	|<NUMTYPES>
	|<IDENTIFIER>
	|<SEMICOLON>
	|<FLOATTYPE>
	|<IDENTIFIER>
	|<SEMICOLON>
	| <BOOLTYPE>	
	|<LPAR>
